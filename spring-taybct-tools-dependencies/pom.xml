        <!--阿里云对象存储-->
        <oss-alibaba.version>3.16.1</oss-alibaba.version>
        <!--对中文多音字处理-->
        <pinyin4j.version>2.5.1</pinyin4j.version>
        <!--    postgresql 驱动   -->
        <postgresql.version>42.7.3</postgresql.version>
        <!--基础工具版本-->
        <project.version>3.2.0</project.version>
        <!--skywalking 日志记录-->
        <skywalking-logback.version>8.14.0</skywalking-logback.version>
        <!--管理和监视Spring Boot的应用程序-->
        <spring-boot-admin.version>3.2.3</spring-boot-admin.version>
        <!--spring boot 版本依赖-->
        <spring-boot.version>3.2.5</spring-boot.version>
        <!--spring cloud 微服务版本依赖-->
        <spring-cloud.version>2023.0.1</spring-cloud.version>
        <!--阿里巴巴版本依赖-->
        <spring-cloud-alibaba.version>2023.0.1.0</spring-cloud-alibaba.version>
        <!--swagger openapi3.0 注解-->
        <swagger-annotations-jakarta.version>2.2.21</swagger-annotations-jakarta.version>
    </properties>

    <dependencyManagement>
        <dependencies>

            <!-- SpringBoot 依赖配置 -->
